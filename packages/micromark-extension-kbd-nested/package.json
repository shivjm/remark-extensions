{
  "name": "micromark-extension-kbd-nested",
  "version": "0.7.1",
  "description": "micromark extension for `kbd` element syntax with nesting, escaping & configurable delimiter",
  "keywords": [
    "micromark",
    "kbd",
    "keyboard",
    "markdown"
  ],
  "homepage": "https://github.com/shivjm/remark-extensions",
  "bugs": {
    "url": "https://github.com/shivjm/remark-extensions/issues"
  },
  "license": "ISC",
  "author": "Shiv Jha-Mathur",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/shivjm/remark-extensions.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "rimraf lib && npm run build",
    "lint": "eslint src/**/*.ts --config ../../eslint.config.mjs",
    "test": "mocha tests/**/*.ts",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "micromark-util-character": "^1.1.0",
    "micromark-util-symbol": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "micromark": "^3.0.7",
    "micromark-util-resolve-all": "^1.0.0",
    "micromark-util-types": "^1.0.1",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "typescript-eslint": "^7.7.1"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "publishConfig": {
    "exports": {
      ".": {
